{
    "name": "ferdousanam/dashboard",
    "description": "This package will create dashboard for project with Metronic Dashboard Template",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ferdous Anam",
            "email": "ferdous.anam@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "gajus/dindent": "^2.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "autoload": {
        "files": [
            "src/app/Helpers/DevOptions.php",
            "src/app/Helpers/Menus.php",
            "src/app/Helpers/ProjectInfo.php",
            "src/app/Helpers/Route.php",
            "src/app/Helpers/Users.php"
        ],
        "classmap": [
            "src/app/Helpers/GetRoute.php"
        ],
        "psr-4": {
            "Anam\\Dashboard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anam\\Dashboard\\DashboardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
